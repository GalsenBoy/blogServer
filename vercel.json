{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js"
    }
  ],
  "env": {
    "DATABASE_USER": "root",
    "DATABASE_PASSWORD": "",
    "DATABASE_HOST": "localhost",
    "DATABASE_NAME": "my_blog"
  }
}